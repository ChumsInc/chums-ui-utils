{
  "name": "@chumsinc/ui-utils",
  "version": "2.1.0",
  "description": "Strongly Typed Utilities for Chums UI",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "src": "./src/index.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "compile": "tsc",
    "build": "npm run clean && npm run compile",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "publish": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChumsInc/chums-ui-utils.git"
  },
  "keywords": [
    "fetch",
    "localstorage",
    "typescript"
  ],
  "author": "Steve Montgomery",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChumsInc/chums-ui-utils/issues"
  },
  "homepage": "https://github.com/ChumsInc/chums-ui-utils#readme",
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "auto-changelog": "^2.4.0",
    "eslint": "^9.18.0",
    "globals": "^16.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "auto-changelog": {
    "template": "changelog-template.hbs",
    "unreleased": true,
    "commitLimit": false,
    "breakingPattern": "BREAKING CHANGE:"
  },
  "dependencies": {
    "react-fast-compare": "^3.2.2"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
